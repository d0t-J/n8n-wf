    {
      "parameters": {
        "jsCode": "// The data from GitHub comes from the first input branch.\nconst githubData = $input.all()[0].json;\n\n// --- FIX ---\n// Get the leetcode code using the full, correct path from the 'Edit Fields1' node.\n// The '?.' prevents errors if any intermediate property (like 'data') doesn't exist.\nconst leetcodeCode = $('Edit Fields1').first().json.content;\n\n// Always check if the variable was found successfully before using it.\nif (!leetcodeCode) {\n  throw new Error(\"Could not find '.code' in the data from the 'Edit Fields1' node. Check the path.\");\n}\n\n// Check if the file already exists on GitHub.\nif (githubData.content) {\n  // The content is base64 encoded, so we need to decode it to a string.\n  const githubCode = Buffer.from(githubData.content, 'base64').toString();\n\n  // Compare the code from GitHub with the new code from LeetCode.\n  if (githubCode.trim() === leetcodeCode.trim()) {\n    // The code is the same, no need to update.\n    githubData.github_status = \"same\";\n  } else {\n    // The code is different, prepare it for an update.\n    githubData.github_status = \"different\";\n    githubData.new_code_content = leetcodeCode;\n  }\n} else {\n  // If no \"content\" key, the file doesn't exist on GitHub.\n  githubData.github_status = \"new\";\n  githubData.new_code_content = leetcodeCode;\n}\n\n// Return the modified data.\nreturn $input.all();"
      },
